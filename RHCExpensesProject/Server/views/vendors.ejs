<!DOCTYPE html>
<html>
<head>
    <title>User Creation</title>
    <link rel="stylesheet" type="text/css" href="/css/addNewUser.css" />
    <link rel="stylesheet" type="text/css" href="/css/basicResultsTable.css" />
    <link rel="shortcut icon" href="/graphics/favicon.png" />
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.3.1/jquery.min.js"></script>
    <script src="/scripts/addNewUser.js"></script>
</head>
<body>
    <div class="contentWrapper">
        <div class="content">
            
            <form>
                <table cellpadding="0" cellspacing="0">
                    <thead>
                        <tr>
                            <th>Vendor Name</th>
                            <th>Street Address</th>
                            <th>City</th> 
                            <th>State</th>
                            <th>Zip Code</th>
                            <th>Payment Terms</th>
                        </tr>
                        <tbody>
                            <% for (var i = 0; i < vendors.length; i++) { %>
                                <tr>
                                    <td><%= vendors[i].vendor_name %></td>
                                    <td><%= vendors[i].vendor_address %></td>
                                    <td><%= vendors[i].vendor_city%></td>
                                    <td><%= vendors[i].vendor_state %></td>
                                    <td><%= vendors[i].vendor_zip %></td>
                                    <td><%= vendors[i].payment_terms %></td>
                                </tr>
                            <% } %>
                        </tbody>
                    </thead>
                </table>
            </form>

        </div>
    </div>

    <a href="logout">Logout</a>
</body>
</html>
